# Telegram API credentials (get from my.telegram.org)
API_ID=your_api_id
API_HASH=your_api_hash

# Bot token (get from @BotFather)
BOT_TOKEN=your_bot_token

# Owner user IDs (comma-separated)
OWNER_IDS=your_telegram_user_id

# Optional: Admin contact username (shown in non-admin messages)
ADMIN_CONTACT_USERNAME=admin

# Optional: Delay settings (in seconds)
JOIN_SECONDS=1
LEAVE_SECONDS=1
AUTO_VIEW_SECONDS=0.5
STATE_TIMEOUT=300

# Optional: Skip cleanup for faster startup (true/false)
SKIP_CLEANUP=false

# Optional: Use lenient mode for less aggressive session cleanup (true/false)
LENIENT_MODE=true
